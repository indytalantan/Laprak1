    "import math \n",
    "\n",
    "biaya_awal = 200000000\n",
    "biaya_yang_ingin_dicapai = 400000000\n",
    "bunganya = 0.1\n",
    "\n",
    "#t = waktu\n",
    "t = round(math.log(biaya_yang_ingin_dicapai / biaya_awal) / math.log(1+bunganya), 2)\n",
    "print('Perlu waktu', t,'tahun untuk mencapai uang')"
   ]
